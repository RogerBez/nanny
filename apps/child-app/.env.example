# Child App Environment Variables
# Copy this file to .env and fill in your values

# API Configuration
EXPO_PUBLIC_API_URL=http://localhost:3000
EXPO_PUBLIC_API_TIMEOUT=30000

# Device & Identification
EXPO_PUBLIC_DEVICE_ID=device-id-placeholder
EXPO_PUBLIC_CHILD_ID=child-id-placeholder

# Encryption Keys (RSA public key for messaging)
EXPO_PUBLIC_RSA_PUBLIC_KEY=your-rsa-public-key-here

# Background Service
EXPO_PUBLIC_BACKGROUND_INTERVAL=300000

# Feature Flags
EXPO_PUBLIC_DEBUG_MODE=false
EXPO_PUBLIC_MOCK_API=false
EXPO_PUBLIC_TEST_MODE=false
